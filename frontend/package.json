{
  "name": "skulkestudio",
  "private": true,
  "packageManager": "yarn@4.10.3",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev --parallel",
    "build": "turbo run build",
    "clean": "turbo run clean && rimraf node_modules",
    "typecheck": "turbo run typecheck",
    "lint": "biome lint && turbo run lint"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.4",
    "rimraf": "^6.0.1",
    "turbo": "^2.5.8",
    "typescript": "^5.9.2"
  }
}
